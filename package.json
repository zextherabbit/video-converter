{
  "name": "video-converter",
  "version": "1.0.0",
  "description": "Video converting service using ffmpeg and nodejs",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start:watch": "nodemon ./dist/src/index.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:watch\"",
    "production": "node ./dist/src/index.js",
    "start": "concurrently \"npm run build\" \"npm run production\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zextherabbit/video-converter.git"
  },
  "keywords": [
    "ffmpeg",
    "video",
    "converter",
    "flv",
    "to",
    "mp4",
    "nodejs",
    "video",
    "converter"
  ],
  "author": "Stefan Djordjevic - zextherabbit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zextherabbit/video-converter/issues"
  },
  "homepage": "https://github.com/zextherabbit/video-converter#readme",
  "dependencies": {
    "concurrently": "^4.1.2",
    "fetch": "^1.1.0",
    "promise-queue": "git+https://git@github.com/zextherabbit/promise-queue"
  }
}
